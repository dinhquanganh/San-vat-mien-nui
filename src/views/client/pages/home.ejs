    <%- include("../layouts/script.ejs") -%>
    <script>
      let imgOwlRecentProduct = document.querySelectorAll(
        '.recent-product-slider .img-block'
      );
      let imgOwlBestSaleProduct = document.querySelectorAll(
        '.best-sell-slider .img-block'
      );

      let resz1 = setInterval(() => {
        if (imgOwlRecentProduct[0].offsetWidth > 10) {
          for (let element of imgOwlRecentProduct) {
            element.style.height = element.offsetWidth + 'px';
          }
          clearInterval(resz1);
        }
      }, 1000);
      let resz2 = setInterval(() => {
        if (imgOwlBestSaleProduct[0].offsetWidth > 10) {
          for (let element of imgOwlBestSaleProduct) {
            element.style.height = element.offsetWidth + 'px';
          }
          clearInterval(resz2);
        }
      }, 1000);
